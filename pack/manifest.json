{
  "manifest_version": 2,
  
  "name": "Didactic Tribble a Chrome Extension",
  "description": "Just playing with the concepts behind Chrome extensions",
  "version": "0.0.1",
  
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click here"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "http://giphy.com/embed/BtIpahM6na1tm?html5=true&playOnHover=true"
  ]
}